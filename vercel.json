{
    "version": 2,
    "builds": [{
        "src": "blogApp/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLmbdaSize": "15mb", "runtime": "python3.9"}
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {"distDi": "staticfiles_build"}
    }],
    "routes": [{
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "blogApp/wsgi.py"
    }]
}